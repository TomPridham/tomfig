source ~/.bash/git-completion.sh
source ~/.bash/gitprompt.sh
GIT_PROMPT_ONLY_IN_REPO=1
GIT_PROMPT_SHOW_UNTRACKED_FILES=normal
alias la='ls -A'
alias gb='git branch'
alias gs='git status'
alias gcam='git commit -am '
alias gu='git push'
alias gp='git pull'
alias cr='cargo run'
export DISPLAY=:0.0
export TERM=xterm-256color
